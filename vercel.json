{
  "functions": {
    "app/**": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/register",
      "destination": "/api/auth/signup"
    },
    {
      "source": "/apipg/:path*",
      "destination": "https://apipg.datazag.com/:path*"
    }
  ]
}
